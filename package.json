    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.7",
    "@storybook/preset-create-react-app": "^4.1.2"
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "@types/react": "^18.0.12"
